restart -f -nowave
view signals wave
config wave -signalnamewidth 1
set NumericStdNoWarnings 1
.main clear


# clk and rstn
add wave clk_tb rstn_tb 
# I2C to ADC
add wave SCL_tb
add wave SDA_tb
# ready
add wave SHDNZ_pin_tb 
add wave SW_vdd_ok_tb

# config state
add wave inst_W/inst_ACFC/state
add wave inst_W/inst_ACFC/next_state


run 2400000 ns